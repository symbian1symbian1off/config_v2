    },
    {
      "listen": "0.0.0.0",
      "port": 8080,
      "protocol": "http",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      },
      "settings": {
        "auth": "noauth",
        "udp": true
      },
      "tag": "http"
    }
  ]
  
